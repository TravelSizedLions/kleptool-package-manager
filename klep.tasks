{
  "link": "ln -s $CODE/kleptool-package-manager/src/index.ts $HOME/.local/bin/klep && chmod +x $HOME/.local/bin/klep",
  "clean-rust": "rm -rf src/rust/pkg src/rust/target",
  "build": "cd src/rust && wasm-pack build --target nodejs && cd ..",
  "build-clean": "klep clean-rust && klep build",
  "readme": "pandoc --pdf-engine=typst README.md -o README.pdf"
}